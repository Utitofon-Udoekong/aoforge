{
  "name": "ao-forge",
  "version": "2.1.2",
  "description": "AO-Forge: CLI tool to create AO-powered applications",
  "type": "module",
  "bin": {
    "ao-forge": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "prepare": "rimraf dist && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "test:cli": "npm run build && node dist/index.js",
    "link:global": "npm run prepare && cross-env-shell \"[[ \\\"$OS\\\" != 'Windows_NT' ]] && chmod +x dist/index.js || exit 0\" && npm link",
    "unlink:global": "npm unlink -g ao-forge",
    "relink:global": "npm run unlink:global && npm run link:global"
  },
  "keywords": [
    "ao-forge",
    "ao",
    "arweave",
    "cli",
    "nextjs",
    "nuxtjs",
    "svelte"
  ],
  "author": "Utitofon Udoekong",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "chalk": "^5.4.1",
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^12.1.0",
    "js-yaml": "^4.1.0",
    "open": "^10.0.0",
    "open-term": "^2.0.4",
    "openai": "^4.73.0",
    "ora": "^8.1.1",
    "which": "^5.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.9.3",
    "@types/which": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.12.3+sha512.cce0f9de9c5a7c95bef944169cc5dfe8741abfb145078c0d508b868056848a87c81e626246cb60967cbd7fd29a6c062ef73ff840d96b3c86c40ac92cf4a813ee"
}
